{
  "name": "quantum-purse",
  "version": "0.0.1",
  "description": "ckb quantum resistant wallet",
  "private": true,
  "scripts": {
    "start": "webpack serve --open --config ./webpack/webpack.dev.js",
    "build:wasm": "cd key-vault && wasm-pack build --release --target web",
    "build:webpack": "webpack --config ./webpack/webpack.prod.js",
    "build": "npm run build:wasm && npm run build:webpack",
    "deploy": "npm run build && node ./src/deploy.js",
    "test:rust": "cargo test",
    "test:js": "npx karma start",
    "test": "npm run test:rust && npm run test:js"
  },
  "author": "tea2x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tea2x/quantum-purse-web-static"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@peculiar/webcrypto": "^1.5.0",
    "@types/chai": "^5.2.0",
    "@types/mocha": "^10.0.10",
    "babel-loader": "^10.0.0",
    "chai": "^5.2.0",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.1.3",
    "fake-indexeddb": "^6.0.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
