{
  "name": "quantum-purse",
  "version": "0.3.0",
  "description": "A CKB SPHINCS+ wallet",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start:web": "webpack serve --config ./webpack/webpack.dev.js",
    "build:web": "webpack --config ./webpack/webpack.prod.js",
    "start:app": "npm run clean && MAIN_NET=false NATIVE_APP=true npm run build:web && electron .",
    "build:app:mac:arm64": "npm run build:app:prepare && npx electron-rebuild --arch arm64 && electron-builder --mac --arm64",
    "build:app:mac:x64": "npm run build:app:prepare && npx electron-rebuild --arch x64 && electron-builder --mac --x64",
    "build:app:linux:x64": "npm run build:app:prepare && npx electron-rebuild --arch x64 && electron-builder --linux --x64",
    "build:app:win:x64": "npm run build:app:prepare && npx electron-rebuild --arch x64 && electron-builder --win --x64",
    "build:app:prepare": "npm run clean && MAIN_NET=true NATIVE_APP=true npm run build:web",
    "clean": "rm -rf dist/ build/",
    "test": "npx karma start"
  },
  "build": {
    "appId": "",
    "productName": "Quantum Purse",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "mac": {
      "target": "dmg",
      "icon": "src/assets/logo.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/logo.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/logo.png"
    }
  },
  "author": "Tea2x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tea2x/quantum-purse-web-static"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@electron/rebuild": "4.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.6.1",
    "@svgr/webpack": "8.1.0",
    "@types/chai": "5.2.2",
    "@types/mocha": "10.0.10",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "babel-loader": "10.0.0",
    "chai": "6.2.0",
    "compression-webpack-plugin": "11.1.0",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.2",
    "electron": "38.2.2",
    "electron-builder": "26.0.12",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.4",
    "karma": "6.4.4",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.4",
    "karma-webpack": "5.0.1",
    "mocha": "11.7.4",
    "react-refresh": "0.18.0",
    "sass": "1.93.2",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "url-loader": "4.1.1",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2",
    "webpack-merge": "6.0.1"
  },
  "dependencies": {
    "@ant-design/icons": "6.1.0",
    "@ant-design/v5-patch-for-react-19": "1.0.3",
    "@ckb-ccc/core": "npm:ckb-ccc-core-light-client-js-patch",
    "@nervosnetwork/ckb-light-client-js": "0.5.3",
    "@rematch/core": "2.2.0",
    "@rematch/loading": "2.1.2",
    "antd": "5.27.5",
    "html5-qrcode": "2.3.8",
    "qrcode.react": "4.2.0",
    "quantum-purse-key-vault": "0.2.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-redux": "9.2.0",
    "react-router-dom": "7.9.4",
    "recharts": "2.15.3"
  },
  "overrides": {
    "@nervosnetwork/ckb-light-client-js": {
      "@ckb-ccc/core": "npm:ckb-ccc-core-light-client-js-patch"
    }
  }
}
